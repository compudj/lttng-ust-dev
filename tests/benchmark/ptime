#!/usr/bin/env python

import sys
import time
import os

def main():
	args = sys.argv[1:]
	if len(args) < 1:
		print("usage: %s COMMAND" % sys.argv[0])
		sys.exit(1)

	cmd = ' '.join(args)

	t1 = time.time()
	os.system(cmd)
	t2 = time.time()

	print("Wall time: " + str(t2-t1))

if __name__ == "__main__":
	main()
